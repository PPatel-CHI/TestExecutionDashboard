    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Electron Main",
            "runtimeExecutable": "${workspaceFolder}/node_modules/.bin/electron",
            "program": "${workspaceFolder}/src/main.js",
            "protocol": "legacy"
        },
        {
            "type": "node",
            "request": "attach",
            "name": "Attach",
            "port": 5858
            ,"outFiles": [
                "\\..\\test\test.js"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch",
            "program": "${workspaceFolder}/src/main.js",
            "outFiles": [
                "\\..\\test\test.js"
            ]
        }
    ]
}